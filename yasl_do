#!yasl
!
!
eval
0:=:1
!
= "+" == ? ( ! eval 1:=:0 + (print(0 exit)))
= "-" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 - (print(0 exit)))
= "*" == ? ( ! eval 1:=:0 * (print(0 exit)))
= "/" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 / (print(0 exit)))
= "%" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 % (print(0 exit)))
= "<" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 < (print(0 exit)))
= ">" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 > (print(0 exit)))
= "<=" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 <= (print(0 exit)))
= ">=" == ? ( ! eval 0:=:2 ! 1:=:0 2:=:0 >= (print(0 exit)))
= "==" == ? ( ! eval 1:=:0 == (print(0 exit)))
= "!=" == ? ( ! eval 1:=:0 != (print(0 exit)))
